<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="menuOneVM" type="com.knobblochsapplication.app.modules.menuone.data.viewmodel.MenuOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearMenuOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearColumnstatelayer"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_4pxv"
            android:layout_marginTop="@dimen/_94pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearNavitem1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_5">
                <androidx.appcompat.widget.SearchView
                    android:background="@drawable/rectangle_bg_white_a700_radius_28"
                    android:gravity="start"
                    android:id="@+id/searchViewStatelayer"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:theme="@style/searchViewstatelayer"
                    app:iconifiedByDefault="false"
                    app:queryBackground="@android:color/transparent"
                    app:queryHint="@string/lbl3" app:searchIcon="@drawable/img_search"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowmenu"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMenu"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearNavitem2"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_6">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtLabel"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{menuOneVM.menuOneModel.txtLabel}"
                        style="@style/txtRobotoromanregular20" tools:text="@string/lbl6"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowcamera"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCamera"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_camera_gray_800"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_camera_gray_800"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearNavitem3"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_6">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtLabelOne"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{menuOneVM.menuOneModel.txtLabelOne}"
                        style="@style/txtRobotoromanregular20" tools:text="@string/lbl20"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowdownload"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageDownload"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_download"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_download"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearNavitem4"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_7">
                    <TextView android:gravity="start"
                        android:id="@+id/txtLanguage"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:text="@{menuOneVM.menuOneModel.txtLanguage}"
                        style="@style/txtRobotoromanregular20" tools:text="@string/msg5"/>
                </LinearLayout>
            </LinearLayout>
            <ImageButton
                android:background="@drawable/rectangle_bg_indigo_300_radius_20"
                android:id="@+id/btnArrowleft"
                android:layout_gravity="end"
                android:layout_height="@dimen/_40pxh"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_101pxv"
                android:layout_width="@dimen/_40pxh"
                android:paddingHorizontal="@dimen/_12pxh"
                android:paddingVertical="@dimen/_12pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_arrowleft_white_a700" tools:ignore="ContentDescription"/>
        </LinearLayout>
    </LinearLayout>
</layout>
