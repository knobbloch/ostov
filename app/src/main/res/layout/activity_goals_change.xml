<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="goalsChangeVM" type="com.knobblochsapplication.app.modules.goalschange.data.viewmodel.GoalsChangeVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearGoalschange"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearBottomSheet"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStyleindigo_50">
                <LinearLayout android:id="@+id/linearColumndraghandle"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <View
                        android:background="@drawable/rectangle_bg_gray_600_7e_radius_2"
                        android:id="@+id/viewDraghandle"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_131pxh"
                        android:layout_marginStart="@dimen/_131pxh" android:layout_width="@dimen/_32pxh"/>
                    <LinearLayout
                        android:id="@+id/linearRowheadlinetext"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHeadlinetext"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.92"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_4pxh"
                            android:singleLine="true"
                            android:text="@{goalsChangeVM.goalsChangeModel.txtHeadlinetext}"
                            style="@style/txtRobotoregular20" tools:text="@string/lbl15"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearContainer"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginStart="@dimen/_138pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.08"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                            <ImageView android:gravity="start"
                                android:id="@+id/imageClose"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_width="match_parent"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_close}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:id="@+id/linearRowlanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.3"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{goalsChangeVM.goalsChangeModel.txtLanguage}"
                    style="@style/txtABeeZeeregular18" tools:text="@string/lbl16"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_50_radius_16"
                    android:id="@+id/viewRectangleThree"
                    android:layout_height="@dimen/_32pxv"
                    android:layout_marginStart="@dimen/_17pxh" android:layout_width="@dimen/_213pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowsix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSix"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.44"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{goalsChangeVM.goalsChangeModel.txtSix}"
                    style="@style/txtABeeZeeregular18" tools:text="@string/lbl17"/>
                <FrameLayout android:id="@+id/frameStackcamera"
                    android:layout_height="@dimen/_32pxv"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_width="@dimen/_176pxh" style="@style/groupStylegray_1">
                    <ImageView android:id="@+id/imageCamera"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_camera"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowseven"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSeven"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.78"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{goalsChangeVM.goalsChangeModel.txtSeven}"
                    style="@style/txtABeeZeeregular18" tools:text="@string/lbl18"/>
                <FrameLayout android:id="@+id/frameCheckboxes"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStylegray_300cornerRadius">
                    <View
                        android:background="@drawable/rectangle_bg_gray_300_radius_5"
                        android:id="@+id/viewCreatefromfra"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginHorizontal="@dimen/_10pxh"
                        android:layout_marginVertical="@dimen/_10pxv" android:layout_width="@dimen/_19pxh"/>
                    <ImageView android:id="@+id/imageFile"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginHorizontal="@dimen/_11pxh"
                        android:layout_marginVertical="@dimen/_11pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_file}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_file"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumneight"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_4">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEight"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{goalsChangeVM.goalsChangeModel.txtEight}"
                    style="@style/txtABeeZeeregular18" tools:text="@string/lbl19"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_50_radius_20"
                    android:id="@+id/viewRectangleTwo"
                    android:layout_height="@dimen/_210pxv"
                    android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_329pxh"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
