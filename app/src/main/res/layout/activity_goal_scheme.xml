<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="goalSchemeVM" type="com.knobblochsapplication.app.modules.goalscheme.data.viewmodel.GoalSchemeVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearGoalscheme"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearGoalscheme1"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_3">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStyleindigo_52">
                <LinearLayout android:id="@+id/linearRowmenu"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_menu_indigo_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_menu_indigo_900"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMygoals"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_75pxh"
                        android:layout_weight="0.57"
                        android:layout_width="0dp"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{goalSchemeVM.goalSchemeModel.txtMygoals}"
                        style="@style/txtABeeZeeregular20" tools:text="@string/lbl"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSeventyFive"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_65pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{goalSchemeVM.goalSchemeModel.txtSeventyFive}"
                        style="@style/txtABeeZeeregular20" tools:text="@string/lbl_75"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/frameFish"
                android:layout_gravity="start"
                android:layout_height="@dimen/_626pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_35pxv" android:layout_width="@dimen/_316pxh">
                <ImageView android:id="@+id/imageSkeleton"
                    android:layout_gravity="center"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_skeleton"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_skeleton"/>
                <LinearLayout android:id="@+id/linearColumnmobile"
                    android:layout_gravity="top|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_27pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowmobile"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMobile"
                            android:layout_height="@dimen/_35pxh"
                            android:layout_width="@dimen/_23pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_mobile"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_mobile"/>
                        <View
                            android:background="@drawable/rectangle_bg_white_a700_radius_13_535"
                            android:id="@+id/viewEllipseEleven"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_45pxh"
                            android:layout_marginTop="@dimen/_6pxv" android:layout_width="@dimen/_27pxh"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtYourgoal"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_34pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{goalSchemeVM.goalSchemeModel.txtYourgoal}"
                        style="@style/txtABeeZeeregular18_1" tools:text="@string/lbl25"/>
                    <LinearLayout android:id="@+id/linearRowtaskone"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_78pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTaskOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_4pxh"
                            android:singleLine="true"
                            android:text="@{goalSchemeVM.goalSchemeModel.txtTaskOne}"
                            style="@style/txtABeeZeeregular18" tools:text="@string/lbl_12"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTaskTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_59pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_4pxh"
                            android:singleLine="true"
                            android:text="@{goalSchemeVM.goalSchemeModel.txtTaskTwo}"
                            style="@style/txtABeeZeeregular18" tools:text="@string/lbl_23"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTaskThree"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_83pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{goalSchemeVM.goalSchemeModel.txtTaskThree}"
                        style="@style/txtABeeZeeregular18" tools:text="@string/lbl_32"/>
                </LinearLayout>
            </FrameLayout>
            <View
                android:background="@drawable/rectangle_bg_gray_900_radius_1"
                android:id="@+id/lineHome"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_2pxv"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_34pxv" android:layout_width="@dimen/_72pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
