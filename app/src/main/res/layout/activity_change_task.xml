<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="changeTaskVM" type="com.knobblochsapplication.app.modules.changetask.data.viewmodel.ChangeTaskVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearChangetask"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearBottomSheet"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                app:contentInsetStart="0dp" style="@style/groupStyleindigo_50">
                <LinearLayout android:id="@+id/linearColumndraghandle"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <View
                        android:id="@+id/viewDraghandle"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_131pxh"
                        android:layout_marginStart="@dimen/_131pxh" android:layout_width="@dimen/_32pxh"/>
                    <LinearLayout
                        android:id="@+id/linearRowheadlinetext"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHeadlinetext"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.93"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_4pxh"
                            android:singleLine="true"
                            android:text="@{changeTaskVM.changeTaskModel.txtHeadlinetext}"
                            style="@style/txtRobotoregular20" tools:text="@string/msg19"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearContainer"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginStart="@dimen/_118pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.07"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                            <ImageView android:gravity="start"
                                android:id="@+id/imageClose"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_width="match_parent"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_close}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:id="@+id/linearRowlanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.3"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{changeTaskVM.changeTaskModel.txtLanguage}"
                    style="@style/txtABeeZeeregular18" tools:text="@string/lbl16"/>
                <View
                    android:id="@+id/viewRectangleThree"
                    android:layout_height="@dimen/_32pxv"
                    android:layout_marginStart="@dimen/_17pxh" android:layout_width="@dimen/_213pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowtwentyeight"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTwentyEight"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.44"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{changeTaskVM.changeTaskModel.txtTwentyEight}"
                    style="@style/txtABeeZeeregular18" tools:text="@string/lbl17"/>
                <FrameLayout android:id="@+id/frameStackcamera"
                    android:layout_height="@dimen/_32pxv"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_width="@dimen/_176pxh" >
                    <ImageView android:id="@+id/imageCamera"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_camera"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowvectorOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowtwentynine"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.85"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTwentyNine"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.76"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{changeTaskVM.changeTaskModel.txtTwentyNine}"
                        style="@style/txtABeeZeeregular18" tools:text="@string/lbl29"/>
                    <ImageButton
                        android:id="@+id/btnVector"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_marginStart="@dimen/_44pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:paddingHorizontal="@dimen/_11pxh"
                        android:paddingVertical="@dimen/_11pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_vector" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <ImageButton
                    android:id="@+id/btnVectorOne"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_marginStart="@dimen/_112pxh"
                    android:layout_width="@dimen/_32pxh"
                    android:paddingHorizontal="@dimen/_11pxh"
                    android:paddingVertical="@dimen/_11pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_vector" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowthirty"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtThirty"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.78"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{changeTaskVM.changeTaskModel.txtThirty}"
                    style="@style/txtABeeZeeregular18" tools:text="@string/lbl18"/>
                <FrameLayout android:id="@+id/frameCheckboxes"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_width="@dimen/_40pxh" >
                    <View
                        android:id="@+id/viewCreatefromfra"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginHorizontal="@dimen/_10pxh"
                        android:layout_marginVertical="@dimen/_10pxv" android:layout_width="@dimen/_19pxh"/>
                    <ImageView android:id="@+id/imageFile"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginHorizontal="@dimen/_11pxh"
                        android:layout_marginVertical="@dimen/_11pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_file}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_file"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnthirtyone"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_4">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtThirtyOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{changeTaskVM.changeTaskModel.txtThirtyOne}"
                    style="@style/txtABeeZeeregular18" tools:text="@string/lbl19"/>
                <View
                    android:id="@+id/viewRectangleTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_131pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_322pxh"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
